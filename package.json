{
  "name": "@mento-protocol/reserve-web",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "tsc",
    "build:next": "next build",
    "deploy": "yarn run kill-dev; yarn run clean-next-cache; yarn run build:next; gcloud app deploy $MANIFEST",
    "deploy:stage": "gcloud config set project celo-reserve-site-281118; NODE_ENV=production DEPLOY_ENV=development MANIFEST=stage.yaml yarn run deploy && yarn view -s stage",
    "deploy:prod": "gcloud config set project celo-reserve-site-281118; NODE_ENV=production DEPLOY_ENV=production MANIFEST=production.yaml yarn run deploy && yarn view",
    "view": "gcloud app browse",
    "kill-dev": "kill -9 $(lsof -ti :3000)",
    "clean-next-cache": "rm -rf .next",
    "postinstall": "yarn secrets:get",
    "start": "next start -p $PORT",
    "test": "jest",
    "lint": "eslint --fix .",
    "pretty": "prettier --config .prettierrc 'src/**/*.{ts, tsx}' '*.yaml' --write",
    "prepare": "husky install",
    "secrets:get": "./scripts/get-secrets.sh"
  },
  "dependencies": {
    "@celo/contractkit": "4.0.0",
    "@headlessui/react": "^1.7.18",
    "@sentry/nextjs": "^7.21.1",
    "@vercel/analytics": "^1.0.1",
    "bignumber.js": "^9.0.2",
    "chart.js": "^4.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "framer-motion": "^11.0.24",
    "front-matter": "^4.0.2",
    "graphql": "^15.7.2",
    "graphql-request": "^3.7.0",
    "markdown-to-jsx": "^7.1.7",
    "mockdate": "^3.0.2",
    "next": "^12.0.9",
    "node-cache": "^5.1.2",
    "raw-loader": "^4.0.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "swr": "^0.5.6",
    "tailwind-merge": "^2.2.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.8.4",
    "@ethersproject/providers": "^5.7.2",
    "@savvywombat/tailwindcss-grid-areas": "^4.0.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/xml2js": "^0.4.8",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^27.0.6",
    "codecov": "^3.6.5",
    "dotenv": "^16.0.3",
    "eslint": "^7.26.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^5.1.3",
    "ethers": "^5.7.2",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "jest-snapshot": "^27.0.6",
    "lint-staged": ">=10",
    "msw": "^0.33.2",
    "node-fetch": "^2.6.7",
    "postcss": "^8.4.38",
    "prettier": "^3.2.1",
